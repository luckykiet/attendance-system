{
  "name": "server",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Tuan Kiet Nguyen",
  "type": "commonjs",
  "main": "app.js",
  "bin": "./bin/www.https.js",
  "scripts": {
    "dev": "NODE_ENV=development && nodemon bin/www.http.js",
    "devhttps": "NODE_ENV=development && nodemon bin/www.https.js",
    "start": "NODE_ENV=production && node bin/www.http.js",
    "starthttps": "NODE_ENV=production && node bin/www.https.js",
    "upgrade-dep": "ncu -u && yarn install",
    "clean-devcert": "node ./scripts/clean-devcert.js",
    "install-devcert": "node ./scripts/install-devcert.js",
    "test": "yarn test:auth && yarn test:mod && yarn test:api && yarn test:unit",
    "test:auth": "NODE_ENV=test && jest --silent --testPathPattern='test/auth' --detectOpenHandles --forceExit",
    "test:mod": "NODE_ENV=test && jest --silent --testPathPattern='test/mod' --detectOpenHandles --forceExit",
    "test:api": "NODE_ENV=test && jest --silent --testPathPattern='test/api' --detectOpenHandles --forceExit",
    "test:unit": "NODE_ENV=test && jest --silent --testPathPattern='test/unit' --detectOpenHandles --forceExit"
  },
  "engines": {
    "node": ">=20.4.0"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "compression": "^1.8.0",
    "connect-mongodb-session": "^5.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "geolib": "^3.3.4",
    "googleapis": "^148.0.0",
    "html-minifier-terser": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "locale": "^0.1.0",
    "lodash": "^4.17.21",
    "mongoose": "8.0.0",
    "morgan": "^1.10.0",
    "nanoid": "3.3.6",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "proper-url-join": "^2.1.2",
    "qrcode": "^1.5.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "devcert": "^1.2.2",
    "eslint": "^9.24.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.1.9",
    "supertest": "^7.1.0"
  },
  "packageManager": "yarn@4.8.1"
}
